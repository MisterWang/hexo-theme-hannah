<%
var categoriesList = list_categories({show_count: false}),
        tagsList = list_tags({show_count: false}),
        archivesList = list_archives({show_count: false});
%>
<% if (!is_year()) { %>
<div id="archive">
    <aside>
        <h2>Archives</h2>
        <% if (archivesList) { %>
        <%- archivesList %>
        <% } else { %>
        <p>None.</p>
        <% } %>
    </aside>
</div>
<% } else { %>
<h1><%= page.month + '/' + page.year %></h1>
<% page.posts.each(function(article) { %>
<div class="archive-item">
    <% if (article.link) { %>
    <!-- external link -->
    <a href="<%= url_for(article.link) %>" target="_blank">
        <%= article.title || 'No Title' %>
    </a>
    <% } else { %>
    <!-- internal link -->
    <a href="<%= url_for(article.path) %>">
        <%= article.title || 'No Title' %>
    </a>
    <% } %>
    <time datetime="<%= date_xml(article.date) %>">
        <%= date(article.date) %>
    </time>
</div>
<% }); %>
<%- partial('partials/pagination') %>
<% } %>

<div class="time-line">
    <div class="time-line-content">
        <div class="time-line-content-circle">
            <span>
                <i class="fh fh-pig"></i>
            </span>
        </div>
        <div class="time-line-content-title">title</div>
        <div class="time-line-content-content">content</div>
    </div>
    <div class="time-line-content"></div>
    <div class="time-line-content"></div>
    <div class="time-line-content"></div>
</div>